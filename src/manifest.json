{
  "name": "GitHub Source Code Minimap",
  "short_name": "GH Minimap",
  "icons": {
    "128": "img/icon-128.png"
  },
  "version": "1.1.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["css/main.css"],
      "js": ["js/main.js"],
      "run_at": "document_end"
    }
  ],
  "homepage_url": "https://github.com/slikts/gh-minimap"
}
